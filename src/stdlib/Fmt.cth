#[inline]
fn str(s: Str) -> Str {
  s
}

#[inline]
fn int(i: Int) -> Str {
  ::Kernel::Builtins::cast_int_to_string(i)
}

#[inline]
fn float(f: Float) -> Str {
  ::Kernel::Builtins::cast_float_to_string(f)
}

#[inline]
fn bool(b: Bool) -> Str {
  match b {
    true => "true",
    false => "false",
  }
}
