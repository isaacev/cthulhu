#[inline]
fn nth(ls: ['a], n: Int) -> Maybe('a) {
  if ::Kernel::Builtins::array_key_exists(n, ls) {
    Maybe::Just(::Kernel::Builtins::subscript(ls, n))
  } else {
    Maybe::None
  }
}

#[inline]
fn length(ls: ['a]) -> Int {
  ::Kernel::Builtins::count(ls)
}

#[inline]
fn iter(f: 'a -> (), l: ['a]) -> () {
  ::Kernel::Builtins::for_each(f, l);
}
