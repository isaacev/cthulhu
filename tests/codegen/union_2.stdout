<?php

namespace Kernel {
  // #[inline]
  // #[construct]
  function _print($a) {
    print($a);
  }
  // #[inline]
  function strval($a) {
    return \strval($a);
  }
  // #[inline]
  function mt_rand($a, $b) {
    return \mt_rand($a, $b);
  }
  // #[inline]
  // #[construct]
  function subscript($a, $b) {
    return $a[$b];
  }
}

namespace union_2 {
  abstract class Foo {}
  class Bar extends \union_2\Foo {
    public $fields;
    function __construct($a) {
      $this->fields = [$a];
    }
  }
  // #[entry]
  function main() {
    new \union_2\Bar(123);
  }
}

namespace {
  \union_2\main();
}
